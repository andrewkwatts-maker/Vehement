{
  "preset": 1,
  "rendering": {
    "backend": 0,
    "resolutionScale": 75,
    "targetFPS": 60,
    "enableAdaptive": true,
    "sdfRasterizer": {
      "tileSize": [16, 16],
      "maxSteps": 96,
      "enableTemporal": true,
      "enableCheckerboard": true,
      "epsilon": 0.001
    },
    "polygonRasterizer": {
      "enableInstancing": true,
      "shadowCascades": 3,
      "msaaSamples": 2,
      "enableOcclusionCulling": true
    },
    "gpuDriven": {
      "enableGPUCulling": true,
      "jobSize": 192,
      "persistentBuffers": true,
      "meshShaders": false
    },
    "asyncCompute": {
      "enable": true,
      "overlap": 60
    }
  },
  "lighting": {
    "clustered": {
      "maxLights": 50000,
      "grid": [24, 14, 32],
      "maxPerCluster": 512,
      "enableOverflow": true
    },
    "shadows": {
      "atlasSize": [8192, 8192],
      "maxMaps": 128,
      "cascadeSplits": [0.1, 0.3, 0.7, 1.0],
      "softSamples": 8,
      "bias": 0.007,
      "contactShadows": false
    },
    "gi": {
      "method": 2,
      "samplesPerPixel": 1,
      "restirReuse": 60,
      "svgfIterations": 4,
      "enableCache": true
    },
    "lightTypes": {
      "point": true,
      "spot": true,
      "directional": true,
      "area": true,
      "emissive": false
    }
  },
  "materials": {
    "physical": {
      "ior": true,
      "dispersion": false,
      "subsurface": true,
      "blackbody": false,
      "clearcoat": true,
      "sheen": false
    },
    "textures": {
      "maxSize": 2048,
      "anisotropic": 8,
      "mipmapBias": 0.0,
      "compression": true,
      "virtual": false
    },
    "shaders": {
      "optimize": true,
      "cache": true,
      "debugInfo": false
    }
  },
  "lod": {
    "quality": 2,
    "bias": 0.0,
    "distances": [8.0, 20.0, 40.0, 80.0],
    "cullingDistance": 150.0,
    "transition": {
      "dithering": true,
      "width": 3.0,
      "hysteresis": 8
    },
    "buildings": {
      "useCustom": false,
      "customDistances": [],
      "customCulling": 0.0
    },
    "units": {
      "useCustom": false,
      "customDistances": [],
      "customCulling": 0.0
    },
    "terrain": {
      "useCustom": false,
      "customDistances": [],
      "customCulling": 0.0
    }
  },
  "caching": {
    "brickCache": {
      "enable": true,
      "atlasSize": [24, 24, 24],
      "maxMemoryMB": 384,
      "deduplication": true
    },
    "shaderCache": {
      "enable": true,
      "path": "cache/shaders/",
      "maxShaders": 512
    },
    "lightCache": {
      "enable": true,
      "updateFrequency": 0,
      "staticCache": true
    }
  },
  "performance": {
    "threadPool": {
      "workerThreads": 6,
      "jobQueueSize": 768
    },
    "memory": {
      "gpuMemoryLimitMB": 0,
      "streamingBudgetMB": 1024
    },
    "profiling": {
      "enable": true,
      "overlay": false,
      "exportCSV": false,
      "outputPath": "profiling/"
    }
  }
}
