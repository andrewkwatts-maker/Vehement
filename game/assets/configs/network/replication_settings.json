          "reliable": true,
          "parameters": [
            { "name": "amount", "type": "float" },
            { "name": "attackerId", "type": "uint64" }
          ]
        },
        {
          "name": "SetRallyPoint",
          "authority": "owner",
          "target": "server",
          "reliable": true,
          "parameters": [
            { "name": "point", "type": "vector3" }
          ]
        }
      ]
    },
    {
