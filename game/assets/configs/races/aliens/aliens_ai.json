    "focus_fire_priority": ["caster", "siege", "ranged", "melee", "worker"],
    "ability_usage": {
      "psionic_storm": {
        "min_targets": 5,
        "priority": "high"
      },
      "blink": {
        "use_to_escape": true,
        "use_to_chase": true,
        "health_threshold": 0.3
      },
      "charge": {
        "auto_use": true
      },
