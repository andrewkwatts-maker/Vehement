    "animationStateMachine": {
        "initialState": "idle",
        "states": {
            "idle": { "animation": "idle", "transitions": [{ "to": "fly", "condition": "isMoving" }, { "to": "attack", "condition": "isAttacking" }, { "to": "ability_charm", "condition": "isCastingQ" }, { "to": "ability_illusion", "condition": "isCastingW" }, { "to": "ability_mana_burn", "condition": "isCastingE" }, { "to": "ability_vengeance", "condition": "isCastingR" }, { "to": "death", "condition": "isDead" }] },
            "fly": { "animation": "fly", "transitions": [{ "to": "idle", "condition": "!isMoving" }, { "to": "attack", "condition": "isAttacking" }, { "to": "ability_charm", "condition": "isCastingQ" }, { "to": "ability_illusion", "condition": "isCastingW" }, { "to": "ability_mana_burn", "condition": "isCastingE" }, { "to": "ability_vengeance", "condition": "isCastingR" }, { "to": "death", "condition": "isDead" }] },
            "attack": { "animation": "attack", "transitions": [{ "to": "idle", "condition": "animationComplete && !isMoving" }, { "to": "fly", "condition": "animationComplete && isMoving" }, { "to": "death", "condition": "isDead" }] },
            "ability_charm": { "animation": "ability_charm", "transitions": [{ "to": "idle", "condition": "animationComplete && !isMoving" }, { "to": "fly", "condition": "animationComplete && isMoving" }, { "to": "death", "condition": "isDead" }] },
            "ability_illusion": { "animation": "ability_illusion", "transitions": [{ "to": "idle", "condition": "animationComplete && !isMoving" }, { "to": "fly", "condition": "animationComplete && isMoving" }, { "to": "death", "condition": "isDead" }] },
            "ability_mana_burn": { "animation": "ability_mana_burn", "transitions": [{ "to": "idle", "condition": "animationComplete && !isMoving" }, { "to": "fly", "condition": "animationComplete && isMoving" }, { "to": "death", "condition": "isDead" }] },
            "ability_vengeance": { "animation": "ability_vengeance", "transitions": [{ "to": "idle", "condition": "animationComplete && !isMoving" }, { "to": "fly", "condition": "animationComplete && isMoving" }, { "to": "death", "condition": "isDead" }] },
            "death": { "animation": "death", "transitions": [] }
        }
    },

