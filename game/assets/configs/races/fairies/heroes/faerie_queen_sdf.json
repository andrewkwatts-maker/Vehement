                { "type": "play_sound", "sound": "mana_burn_cast" },
                { "type": "spend_mana", "amount": 60 },
                { "type": "start_cooldown", "ability": "mana_burn" }
            ]
        },
        "on_ability_r": {
            "conditions": [{ "type": "has_mana", "amount": 200 }, { "type": "cooldown_ready", "ability": "vengeance" }],
            "actions": [
