{
  "type": "project",
  "version": "1.0",
  "name": "Default Project",
  "description": "Complete example project showcasing all Nova3D Engine features",
  "author": "Nova3D Team",
  "createdDate": "2025-01-01",

  "worldConfig": "world.json",
  "playerStartConfig": "player_start.json",
  "gameRulesConfig": "game_rules.json",

  "assetDirectories": [
    "assets/materials",
    "assets/lights",
    "assets/models",
    "assets/animations",
    "assets/scripts",
    "assets/shaders",
    "assets/audio",
    "assets/vfx"
  ],

  "startupScenes": [
    "scenes/main_menu.scene"
  ],

  "renderSettings": {
    "defaultResolution": [1920, 1080],
    "vsync": true,
    "targetFPS": 60,
    "antialiasing": "MSAA4x",
    "shadowQuality": "high",
    "enableRaytracing": false
  },

  "physics": {
    "gravity": [0.0, -9.81, 0.0],
    "fixedTimestep": 0.02,
    "substeps": 4
  },

  "audio": {
    "masterVolume": 1.0,
    "musicVolume": 0.7,
    "sfxVolume": 0.8
  }
}
