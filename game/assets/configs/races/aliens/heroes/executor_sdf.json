    "behaviors": {
        "on_spawn": {
            "actions": [
                { "type": "play_sound", "sound": "hero_executor_spawn" },
                { "type": "spawn_effect", "effect": "psionic_arrival", "duration": 2.0 },
                { "type": "say", "text": "I am the blade of the Collective." }
            ]
        },
        "on_attack": {
            "actions": [
                { "type": "play_animation", "animation": "attack" },
                { "type": "spawn_effect", "effect": "psi_blade_trail", "target": "attack_target" },
                { "type": "play_sound", "sound": "executor_attack" }
            ]
        },
        "on_ability_q": {
            "conditions": [
                { "type": "has_energy", "amount": 25 },
                { "type": "cooldown_ready", "ability": "psi_blades" }
            ],
            "actions": [
                { "type": "play_animation", "animation": "ability_psi_blades" },
                { "type": "spawn_effect", "effect": "psi_blades_enhance", "target": "self", "duration": 5.0 },
                { "type": "apply_buff", "buff": "psi_blades", "duration": 5.0 },
                { "type": "play_sound", "sound": "psi_blades_activate" },
                { "type": "spend_energy", "amount": 25 },
                { "type": "start_cooldown", "ability": "psi_blades" }
            ]
        },
        "on_ability_w": {
            "conditions": [
                { "type": "has_energy", "amount": 50 },
                { "type": "cooldown_ready", "ability": "blink" }
            ],
            "actions": [
                { "type": "play_animation", "animation": "ability_blink" },
                { "type": "spawn_effect", "effect": "blink_departure", "target": "self" },
                { "type": "teleport", "destination": "ability_target" },
                { "type": "spawn_effect", "effect": "blink_arrival", "target": "self", "duration": 0.5 },
                { "type": "area_damage", "target": "ability_target", "radius": 2.5, "damage": 50, "filter": "enemy" },
                { "type": "play_sound", "sound": "blink_teleport" },
                { "type": "spend_energy", "amount": 50 },
                { "type": "start_cooldown", "ability": "blink" }
            ]
        },
        "on_ability_e": {
            "conditions": [
                { "type": "has_energy", "amount": 60 },
                { "type": "cooldown_ready", "ability": "feedback" }
            ],
            "actions": [
                { "type": "play_animation", "animation": "ability_feedback" },
                { "type": "spawn_effect", "effect": "feedback_beam", "target": "ability_target" },
                { "type": "drain_energy", "target": "ability_target", "damageMultiplier": 0.75 },
                { "type": "play_sound", "sound": "feedback_drain" },
                { "type": "spend_energy", "amount": 60 },
                { "type": "start_cooldown", "ability": "feedback" }
            ]
        },
        "on_ability_r": {
            "conditions": [
                { "type": "has_energy", "amount": 150 },
                { "type": "cooldown_ready", "ability": "void_prison" }
            ],
            "actions": [
                { "type": "play_animation", "animation": "ability_void_prison" },
                { "type": "spawn_effect", "effect": "void_prison_field", "target": "ability_target", "duration": 4.0 },
                { "type": "area_stasis", "target": "ability_target", "radius": 2.5, "duration": 4.0, "filter": "enemy" },
                { "type": "play_sound", "sound": "void_prison_cast" },
                { "type": "spend_energy", "amount": 150 },
                { "type": "start_cooldown", "ability": "void_prison" }
            ]
        },
        "on_kill": {
            "actions": [
                { "type": "gain_experience", "amount": "target.experience_value" },
                { "type": "random_say", "texts": ["None shall stand before us.", "Your resistance ends here.", "For the Collective."], "chance": 0.3 }
            ]
        },
        "on_death": {
            "actions": [
                { "type": "play_animation", "animation": "death" },
                { "type": "spawn_effect", "effect": "psionic_explosion", "duration": 2.0 },
                { "type": "play_sound", "sound": "executor_death" },
                { "type": "say", "text": "I... return... to the void..." }
            ]
        }
