{
  "version": "1.0.0",
  "entities": [
    {
      "name": "NetworkedUnit",
      "networkId": 1,
      "relevanceMode": "distance",
      "relevanceRadius": 500.0,
      "dormancyEnabled": true,
      "dormancyTimeout": 5.0,
      "snapshotRate": 20,
      "properties": [
        {
          "name": "position",
          "type": "vector3",
          "replicationMode": "always",
          "interpolation": "smooth",
          "compression": "quantize",
          "quantizationBits": 16,
          "priority": 255
        },
        {
          "name": "rotation",
          "type": "quaternion",
          "replicationMode": "onchange",
          "interpolation": "smooth",
          "compression": "quantize",
          "quantizationBits": 10,
          "priority": 200
        },
        {
          "name": "velocity",
          "type": "vector3",
          "replicationMode": "always",
          "interpolation": "linear",
          "compression": "quantize",
          "quantizationBits": 12,
          "priority": 240
        },
        {
          "name": "health",
          "type": "float",
          "replicationMode": "onchange",
          "interpolation": "snap",
          "compression": "quantize",
          "quantizationBits": 8,
          "priority": 220
        },
        {
          "name": "maxHealth",
          "type": "float",
          "replicationMode": "initial",
          "priority": 100
        },
        {
          "name": "state",
          "type": "int32",
          "replicationMode": "onchange",
          "priority": 200
        },
        {
          "name": "targetId",
          "type": "uint64",
          "replicationMode": "onchange",
          "priority": 180
        },
        {
          "name": "animationState",
          "type": "int32",
          "replicationMode": "onchange",
          "priority": 150
        },
        {
          "name": "teamId",
          "type": "int32",
          "replicationMode": "initial",
          "priority": 100
        }
      ],
      "rpcs": [
        {
          "name": "TakeDamage",
          "authority": "server",
          "target": "all_clients",
          "reliable": true,
          "parameters": [
            { "name": "amount", "type": "float" },
            { "name": "attackerId", "type": "uint64" },
            { "name": "damageType", "type": "int32" }
          ]
        },
        {
          "name": "PlayEffect",
          "authority": "server",
          "target": "all_clients",
          "reliable": false,
          "parameters": [
            { "name": "effectId", "type": "int32" },
            { "name": "position", "type": "vector3" }
          ]
        },
        {
          "name": "RequestMove",
          "authority": "owner",
          "target": "server",
          "reliable": true,
          "rateLimit": 30,
          "parameters": [
            { "name": "destination", "type": "vector3" },
            { "name": "timestamp", "type": "float" }
          ],
          "validation": "distance(destination, position) < 1000"
        }
      ],
      "groups": [
        {
          "name": "combat",
          "properties": ["health", "state", "targetId"],
          "condition": "inCombat"
        },
        {
          "name": "movement",
          "properties": ["position", "rotation", "velocity"],
          "condition": "isMoving"
        }
      ]
    },
    {
      "name": "NetworkedBuilding",
      "networkId": 2,
      "relevanceMode": "always",
      "dormancyEnabled": true,
      "dormancyTimeout": 30.0,
      "snapshotRate": 5,
