{
  "preset": 0,
  "rendering": {
    "backend": 0,
    "resolutionScale": 50,
    "targetFPS": 30,
    "enableAdaptive": true,
    "sdfRasterizer": {
      "tileSize": [16, 16],
      "maxSteps": 64,
      "enableTemporal": true,
      "enableCheckerboard": true,
      "epsilon": 0.002
    },
    "polygonRasterizer": {
      "enableInstancing": true,
      "shadowCascades": 2,
      "msaaSamples": 0,
      "enableOcclusionCulling": false
    },
    "gpuDriven": {
      "enableGPUCulling": false,
      "jobSize": 128,
      "persistentBuffers": false,
      "meshShaders": false
    },
    "asyncCompute": {
      "enable": false,
      "overlap": 50
    }
  },
  "lighting": {
    "clustered": {
      "maxLights": 10000,
      "grid": [16, 9, 24],
      "maxPerCluster": 256,
      "enableOverflow": false
    },
    "shadows": {
      "atlasSize": [4096, 4096],
      "maxMaps": 64,
      "cascadeSplits": [0.2, 0.5, 1.0],
      "softSamples": 4,
      "bias": 0.01,
      "contactShadows": false
    },
    "gi": {
      "method": 0,
      "samplesPerPixel": 1,
      "restirReuse": 0,
      "svgfIterations": 0,
      "enableCache": false
    },
    "lightTypes": {
      "point": true,
      "spot": true,
      "directional": true,
      "area": false,
      "emissive": false
    }
  },
  "materials": {
    "physical": {
      "ior": false,
      "dispersion": false,
      "subsurface": false,
      "blackbody": false,
      "clearcoat": false,
      "sheen": false
    },
    "textures": {
      "maxSize": 1024,
      "anisotropic": 4,
      "mipmapBias": 0.5,
      "compression": true,
      "virtual": false
    },
    "shaders": {
      "optimize": true,
      "cache": true,
      "debugInfo": false
    }
  },
  "lod": {
    "quality": 1,
    "bias": 1.0,
    "distances": [5.0, 15.0, 30.0, 60.0],
    "cullingDistance": 100.0,
    "transition": {
      "dithering": false,
      "width": 2.0,
      "hysteresis": 5
    },
    "buildings": {
      "useCustom": false,
      "customDistances": [],
      "customCulling": 0.0
    },
    "units": {
      "useCustom": false,
      "customDistances": [],
      "customCulling": 0.0
    },
    "terrain": {
      "useCustom": false,
      "customDistances": [],
      "customCulling": 0.0
    }
  },
  "caching": {
    "brickCache": {
      "enable": true,
      "atlasSize": [16, 16, 16],
      "maxMemoryMB": 256,
      "deduplication": true
    },
    "shaderCache": {
      "enable": true,
      "path": "cache/shaders/",
      "maxShaders": 256
    },
    "lightCache": {
      "enable": false,
      "updateFrequency": 1,
      "staticCache": false
    }
  },
  "performance": {
    "threadPool": {
      "workerThreads": 4,
      "jobQueueSize": 512
    },
    "memory": {
      "gpuMemoryLimitMB": 0,
      "streamingBudgetMB": 512
    },
    "profiling": {
      "enable": false,
      "overlay": false,
      "exportCSV": false,
      "outputPath": "profiling/"
    }
  }
}
