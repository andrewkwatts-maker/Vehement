        "position": [700, 175],
        "parameters": {
          "iterations": 1000,
          "rainAmount": 0.01,
          "evaporation": 0.01,
          "sedimentCapacity": 4.0,
          "erosionStrength": 0.3,
          "depositionStrength": 0.3
