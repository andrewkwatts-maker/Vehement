      },
      {
        "id": "thermal_erosion",
        "type": "ThermalErosion",
        "position": [900, 175],
        "parameters": {
          "iterations": 50,
          "talusAngle": 0.7,
          "strength": 0.5
        }
      },
      {
        "id": "climate",
        "type": "Climate",
        "position": [500, 400],
        "parameters": {
          "latitude": 45.0,
          "windPattern": "westerlies"
        }
      },
      {
        "id": "biome",
        "type": "Biome",
        "position": [700, 400],
        "parameters": {}
      }
    ],
    "connections": [
      {
        "from": "base_noise.value",
        "to": "blend_terrain.inputA"
      },
      {
        "from": "mountain_noise.value",
        "to": "ridge_transform.heightmap"
      },
      {
        "from": "ridge_transform.ridgedHeightmap",
        "to": "blend_terrain.inputB"
      },
      {
        "from": "blend_terrain.result",
        "to": "hydraulic_erosion.heightmap"
      },
      {
        "from": "hydraulic_erosion.erodedHeightmap",
        "to": "thermal_erosion.heightmap"
      },
      {
        "from": "thermal_erosion.erodedHeightmap",
        "to": "climate.heightmap"
      },
      {
        "from": "climate.temperature",
        "to": "biome.temperature"
      },
      {
        "from": "climate.precipitation",
        "to": "biome.precipitation"
      }
    ]
  },

