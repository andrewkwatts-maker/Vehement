                { "type": "play_sound", "sound": "vengeance_summon" },
                { "type": "spend_mana", "amount": 200 },
                { "type": "start_cooldown", "ability": "vengeance" }
            ]
        },
        "on_kill": {
            "actions": [
                { "type": "gain_experience", "amount": "target.experience_value" },
                { "type": "random_say", "texts": ["Such is the fate of those who defy me.", "The Court is absolute.", "Bow before your Queen."], "chance": 0.3 }
