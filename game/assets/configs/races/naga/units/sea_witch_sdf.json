    "animationStateMachine": {
        "initialState": "idle",
        "states": {
            "idle": { "animation": "idle", "transitions": [{ "to": "slither", "condition": "isMoving" }, { "to": "attack", "condition": "isAttacking" }, { "to": "cast", "condition": "isCasting" }, { "to": "swim", "condition": "isSwimming" }, { "to": "death", "condition": "isDead" }] },
            "slither": { "animation": "slither", "transitions": [{ "to": "idle", "condition": "!isMoving" }, { "to": "attack", "condition": "isAttacking" }, { "to": "swim", "condition": "isSwimming" }, { "to": "death", "condition": "isDead" }] },
            "swim": { "animation": "swim", "transitions": [{ "to": "slither", "condition": "!isSwimming && isMoving" }, { "to": "idle", "condition": "!isSwimming && !isMoving" }, { "to": "attack", "condition": "isAttacking" }, { "to": "death", "condition": "isDead" }] },
            "attack": { "animation": "attack", "transitions": [{ "to": "idle", "condition": "animationComplete && !isMoving" }, { "to": "slither", "condition": "animationComplete && isMoving" }, { "to": "death", "condition": "isDead" }] },
            "cast": { "animation": "cast", "transitions": [{ "to": "idle", "condition": "animationComplete" }, { "to": "death", "condition": "isDead" }] },
            "death": { "animation": "death", "transitions": [] }
        }
    },

