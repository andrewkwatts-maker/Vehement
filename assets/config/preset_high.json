{
  "preset": 2,
  "rendering": {
    "backend": 0,
    "resolutionScale": 100,
    "targetFPS": 60,
    "enableAdaptive": true,
    "sdfRasterizer": {
      "tileSize": [16, 16],
      "maxSteps": 128,
      "enableTemporal": true,
      "enableCheckerboard": false,
      "epsilon": 0.001
    },
    "polygonRasterizer": {
      "enableInstancing": true,
      "shadowCascades": 4,
      "msaaSamples": 4,
      "enableOcclusionCulling": true
    },
    "gpuDriven": {
      "enableGPUCulling": true,
      "jobSize": 256,
      "persistentBuffers": true,
      "meshShaders": false
    },
    "asyncCompute": {
      "enable": true,
      "overlap": 80
    }
  },
  "lighting": {
    "clustered": {
      "maxLights": 100000,
      "grid": [32, 18, 48],
      "maxPerCluster": 1024,
      "enableOverflow": true
    },
    "shadows": {
      "atlasSize": [16384, 16384],
      "maxMaps": 256,
      "cascadeSplits": [0.1, 0.25, 0.5, 1.0],
      "softSamples": 16,
      "bias": 0.005,
      "contactShadows": true
    },
    "gi": {
      "method": 3,
      "samplesPerPixel": 1,
      "restirReuse": 80,
      "svgfIterations": 5,
      "enableCache": true
    },
    "lightTypes": {
      "point": true,
      "spot": true,
      "directional": true,
      "area": true,
      "emissive": true
    }
  },
  "materials": {
    "physical": {
      "ior": true,
      "dispersion": true,
      "subsurface": true,
      "blackbody": true,
      "clearcoat": true,
      "sheen": true
    },
    "textures": {
      "maxSize": 4096,
      "anisotropic": 16,
      "mipmapBias": 0.0,
      "compression": true,
      "virtual": false
    },
    "shaders": {
      "optimize": true,
      "cache": true,
      "debugInfo": false
    }
  },
  "lod": {
    "quality": 3,
    "bias": 0.0,
    "distances": [10.0, 25.0, 50.0, 100.0],
    "cullingDistance": 200.0,
    "transition": {
      "dithering": true,
      "width": 5.0,
      "hysteresis": 10
    },
    "buildings": {
      "useCustom": false,
      "customDistances": [],
      "customCulling": 0.0
    },
    "units": {
      "useCustom": false,
      "customDistances": [],
      "customCulling": 0.0
    },
    "terrain": {
      "useCustom": false,
      "customDistances": [],
      "customCulling": 0.0
    }
  },
  "caching": {
    "brickCache": {
      "enable": true,
      "atlasSize": [32, 32, 32],
      "maxMemoryMB": 512,
      "deduplication": true
    },
    "shaderCache": {
      "enable": true,
      "path": "cache/shaders/",
      "maxShaders": 1000
    },
    "lightCache": {
      "enable": true,
      "updateFrequency": 0,
      "staticCache": true
    }
  },
  "performance": {
    "threadPool": {
      "workerThreads": 8,
      "jobQueueSize": 1024
    },
    "memory": {
      "gpuMemoryLimitMB": 0,
      "streamingBudgetMB": 2048
    },
    "profiling": {
      "enable": true,
      "overlay": true,
      "exportCSV": false,
      "outputPath": "profiling/"
    }
  }
}
