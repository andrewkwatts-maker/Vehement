    "animation_state_machine": {
        "states": [
            {"name": "idle", "animation": "idle", "default": true},
            {"name": "walk", "animation": "walk"},
            {"name": "attack", "animation": "attack"},
            {"name": "shield_wall", "animation": "ability_shield_wall"},
            {"name": "rallying_cry", "animation": "ability_rallying_cry"},
            {"name": "blade_storm", "animation": "ability_blade_storm"},
            {"name": "death", "animation": "death"}
        ],
        "transitions": [
            {"from": "idle", "to": "walk", "condition": "speed > 0.1", "blend_time": 0.2},
            {"from": "walk", "to": "idle", "condition": "speed < 0.1", "blend_time": 0.2},
            {"from": "idle", "to": "attack", "condition": "trigger_attack", "blend_time": 0.1},
            {"from": "walk", "to": "attack", "condition": "trigger_attack", "blend_time": 0.1},
            {"from": "attack", "to": "idle", "condition": "animation_complete", "blend_time": 0.2},
            {"from": "any", "to": "shield_wall", "condition": "trigger_ability_q", "blend_time": 0.1},
            {"from": "shield_wall", "to": "idle", "condition": "animation_complete", "blend_time": 0.3},
            {"from": "any", "to": "rallying_cry", "condition": "trigger_ability_w", "blend_time": 0.1},
            {"from": "rallying_cry", "to": "idle", "condition": "animation_complete", "blend_time": 0.2},
            {"from": "any", "to": "blade_storm", "condition": "trigger_ability_e", "blend_time": 0.15},
            {"from": "blade_storm", "to": "idle", "condition": "ability_end", "blend_time": 0.3},
            {"from": "any", "to": "death", "condition": "health <= 0", "blend_time": 0.1}
        ]
    },

    "behavior": {
        "ai_type": "hero_commander",
        "behavior_tree": "ai/behaviors/hero_commander.bt",
        "scripts": {
            "on_spawn": {
                "actions": [
                    {"type": "play_animation", "animation": "idle"},
                    {"type": "spawn_effect", "effect": "hero_spawn_light"},
                    {"type": "play_sound", "sound": "hero_ready"}
                ]
            },
            "on_attack": {
                "actions": [
                    {"type": "play_animation", "animation": "attack"},
                    {"type": "deal_damage", "delay": 0.35}
                ]
            },
            "on_ability_q": {
                "conditions": [
                    {"type": "has_mana", "amount": "ability_cost"},
                    {"type": "ability_ready", "ability": "shield_wall"}
                ],
                "actions": [
                    {"type": "consume_mana", "amount": "ability_cost"},
                    {"type": "play_animation", "animation": "ability_shield_wall"},
                    {"type": "apply_buff", "buff": "shield_wall_buff", "duration": "ability_duration"},
                    {"type": "spawn_effect", "effect": "shield_barrier"},
                    {"type": "start_cooldown", "ability": "shield_wall"}
                ]
            },
            "on_ability_w": {
                "conditions": [
                    {"type": "has_mana", "amount": "ability_cost"},
                    {"type": "ability_ready", "ability": "rallying_cry"}
                ],
                "actions": [
                    {"type": "consume_mana", "amount": "ability_cost"},
                    {"type": "play_animation", "animation": "ability_rallying_cry"},
                    {"type": "apply_aura_buff", "buff": "rally_buff", "radius": "ability_radius", "duration": "ability_duration"},
                    {"type": "spawn_effect", "effect": "rally_wave"},
                    {"type": "start_cooldown", "ability": "rallying_cry"}
                ]
            },
            "on_ability_e": {
                "conditions": [
                    {"type": "has_mana", "amount": "ability_cost"},
                    {"type": "ability_ready", "ability": "blade_storm"}
                ],
                "actions": [
                    {"type": "consume_mana", "amount": "ability_cost"},
                    {"type": "set_state", "state": "channeling"},
                    {"type": "play_animation", "animation": "ability_blade_storm"},
                    {"type": "apply_buff", "buff": "magic_immune"},
                    {"type": "start_aoe_damage", "radius": "ability_radius", "dps": "ability_dps", "duration": "ability_duration"},
                    {"type": "spawn_effect", "effect": "blade_storm_vortex"},
                    {"type": "start_cooldown", "ability": "blade_storm"}
                ]
            },
            "on_ability_r": {
                "conditions": [
                    {"type": "has_mana", "amount": "ability_cost"},
                    {"type": "ability_ready", "ability": "avatar_of_war"},
                    {"type": "min_level", "level": 6}
                ],
                "actions": [
                    {"type": "consume_mana", "amount": "ability_cost"},
                    {"type": "apply_buff", "buff": "avatar_buff", "duration": "ability_duration"},
                    {"type": "apply_aura_buff", "buff": "avatar_aura", "radius": 15, "duration": "ability_duration"},
                    {"type": "scale_model", "scale": 1.3},
                    {"type": "spawn_effect", "effect": "avatar_transformation"},
                    {"type": "play_sound", "sound": "avatar_roar"},
                    {"type": "start_cooldown", "ability": "avatar_of_war"}
                ]
            },
            "on_kill": {
                "actions": [
                    {"type": "gain_experience", "amount": "kill_xp"},
                    {"type": "trigger_passive", "passive": "commanding_presence_kill_bonus"}
                ]
            },
            "on_death": {
                "actions": [
                    {"type": "play_animation", "animation": "death"},
                    {"type": "spawn_effect", "effect": "hero_death_light"},
                    {"type": "play_sound", "sound": "hero_death"},
                    {"type": "drop_items", "drop_chance": 0.0}
                ]
            }
        },

        "passive_aura": {
            "name": "Commanding Presence",
            "radius": 12.0,
            "effects": [
                {"type": "armor_bonus", "value": 2, "target": "allies"},
                {"type": "damage_bonus_vs_target", "value": 0.1, "condition": "attacking_self"}
            ],
            "visual": {
                "ground_decal": "effects/commander_aura_decal",
                "particle": "effects/commander_aura_particles"
            }
        }
    },

    "effects": {
        "shield_barrier": {
            "type": "shield",
            "shape": "hemisphere",
            "radius": 1.5,
            "color": [0.8, 0.7, 0.2, 0.6],
            "emission": 0.5,
            "duration": "buff_duration",
            "pulse_rate": 0.5
