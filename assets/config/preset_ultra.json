{
  "preset": 3,
  "rendering": {
    "backend": 0,
    "resolutionScale": 100,
    "targetFPS": 120,
    "enableAdaptive": true,
    "sdfRasterizer": {
      "tileSize": [16, 16],
      "maxSteps": 256,
      "enableTemporal": true,
      "enableCheckerboard": false,
      "epsilon": 0.0005
    },
    "polygonRasterizer": {
      "enableInstancing": true,
      "shadowCascades": 6,
      "msaaSamples": 8,
      "enableOcclusionCulling": true
    },
    "gpuDriven": {
      "enableGPUCulling": true,
      "jobSize": 256,
      "persistentBuffers": true,
      "meshShaders": true
    },
    "asyncCompute": {
      "enable": true,
      "overlap": 90
    }
  },
  "lighting": {
    "clustered": {
      "maxLights": 250000,
      "grid": [48, 27, 64],
      "maxPerCluster": 2048,
      "enableOverflow": true
    },
    "shadows": {
      "atlasSize": [32768, 32768],
      "maxMaps": 512,
      "cascadeSplits": [0.05, 0.15, 0.35, 0.65, 0.85, 1.0],
      "softSamples": 32,
      "bias": 0.003,
      "contactShadows": true
    },
    "gi": {
      "method": 3,
      "samplesPerPixel": 2,
      "restirReuse": 90,
      "svgfIterations": 7,
      "enableCache": true
    },
    "lightTypes": {
      "point": true,
      "spot": true,
      "directional": true,
      "area": true,
      "emissive": true
    }
  },
  "materials": {
    "physical": {
      "ior": true,
      "dispersion": true,
      "subsurface": true,
      "blackbody": true,
      "clearcoat": true,
      "sheen": true
    },
    "textures": {
      "maxSize": 8192,
      "anisotropic": 16,
      "mipmapBias": -0.25,
      "compression": true,
      "virtual": true
    },
    "shaders": {
      "optimize": true,
      "cache": true,
      "debugInfo": false
    }
  },
  "lod": {
    "quality": 4,
    "bias": -0.5,
    "distances": [15.0, 40.0, 80.0, 150.0],
    "cullingDistance": 300.0,
    "transition": {
      "dithering": true,
      "width": 8.0,
      "hysteresis": 15
    },
    "buildings": {
      "useCustom": false,
      "customDistances": [],
      "customCulling": 0.0
    },
    "units": {
      "useCustom": false,
      "customDistances": [],
      "customCulling": 0.0
    },
    "terrain": {
      "useCustom": false,
      "customDistances": [],
      "customCulling": 0.0
    }
  },
  "caching": {
    "brickCache": {
      "enable": true,
      "atlasSize": [48, 48, 48],
      "maxMemoryMB": 1024,
      "deduplication": true
    },
    "shaderCache": {
      "enable": true,
      "path": "cache/shaders/",
      "maxShaders": 2000
    },
    "lightCache": {
      "enable": true,
      "updateFrequency": 0,
      "staticCache": true
    }
  },
  "performance": {
    "threadPool": {
      "workerThreads": 0,
      "jobQueueSize": 2048
    },
    "memory": {
      "gpuMemoryLimitMB": 0,
      "streamingBudgetMB": 4096
    },
    "profiling": {
      "enable": true,
      "overlay": true,
      "exportCSV": true,
      "outputPath": "profiling/"
    }
  }
}
